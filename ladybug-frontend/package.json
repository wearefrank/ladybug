{
  "name": "@wearefrank/ladybug",
  "version": "${project.version}",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config src/proxy.conf.json",
    "startWithFF": "ng serve --proxy-config src/proxy.ff.conf.json --host 0.0.0.0",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "start-server-and-test 'ng serve' 4200 cypress:run",
    "e2e-interactive": "start-server-and-test 'ng serve' 4200 cypress:open",
    "e2e-interactive-jar": "cypress open --config baseUrl=http://localhost/ladybug/",
    "e2e-jar": "cypress run --config baseUrl=http://localhost/ladybug/",
    "lint": "npx eslint",
    "lintQuiet": "npx eslint --quiet"
  },
  "dependencies": {
    "@angular/animations": "20.3.16",
    "@angular/cdk": "20.2.14",
    "@angular/common": "20.3.16",
    "@angular/compiler": "20.3.16",
    "@angular/core": "20.3.16",
    "@angular/forms": "20.3.16",
    "@angular/localize": "20.3.16",
    "@angular/material": "20.2.14",
    "@angular/platform-browser": "20.3.16",
    "@angular/platform-browser-dynamic": "20.3.16",
    "@angular/router": "20.3.16",
    "@ng-bootstrap/ng-bootstrap": "19.0.1",
    "@popperjs/core": "^2.11.8",
    "ace-builds": "^1.43.6",
    "angular-split": "^20.0.0",
    "bootstrap": "^4.6.2",
    "bootstrap-icons": "^1.13.1",
    "bower": "1.8.14",
    "chai-colors": "^1.0.1",
    "diff-match-patch": "1.0.5",
    "font-awesome": "^4.7.0",
    "monaco-editor": "0.55.1",
    "ng-simple-file-tree": "0.1.30",
    "path": "^0.12.7",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "xml-beautifier": "0.5.0",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.2.1",
    "@angular-eslint/builder": "20.5.0",
    "@angular-eslint/eslint-plugin": "20.5.0",
    "@angular-eslint/eslint-plugin-template": "20.3.0",
    "@angular-eslint/schematics": "20.3.0",
    "@angular-eslint/template-parser": "20.3.0",
    "@angular/cli": "20.2.1",
    "@angular/compiler-cli": "20.3.16",
    "@eslint/js": "^9.36.0",
    "@prettier/plugin-xml": "^3.4.2",
    "@types/bootstrap": "^4.6.6",
    "@types/diff-match-patch": "1.0.36",
    "@types/jasmine": "6.0.0",
    "@types/node": "25.1.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.54.0",
    "chai-match": "^1.1.1",
    "cypress": "15.9.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unicorn": "^62.0.0",
    "jasmine-core": "5.12.1",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "prettier": "^3.7.3",
